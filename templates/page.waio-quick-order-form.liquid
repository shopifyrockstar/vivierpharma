<script>
  var max_page = {{max_page}}
      //       console.log('max_page',max_page);
      var c_page = {{c_page}}
        var curr = $('.cs_pagination.active').text();
  var pag_data = pageNumbers(parseInt(max_page), parseInt(curr));
  var g = [];
  var y = 0;
  var tot = $(pag_data).length;
  if(max_page < 2 ){
    $('.custom_pagination').hide();
  }else{
    $('.custom_pagination').show();
  }
  $(pag_data).each(function(e){

    var dot_chk = pag_data[e]+1;
    var next_val = pag_data[e+1];


    if(c_page == pag_data[e]){
      g = g + ('<div class="cs_pagination active"><a href="/pages/{{ page.handle }}">'+pag_data[e]+'</a></div>');
               } else{
               g = g + ('<div class="cs_pagination"><a href="/pages/{{ page.handle }}/page-'+pag_data[e]+'">'+pag_data[e]+'</a></div>' );
               if(dot_chk != next_val){
        if (e != tot - 1) {
          g = g + ('<div class="cs_pagination">...</div>');
        }
      }

    }

  })

  $('.custom_pagination').html('<div class="prev_page {% if c_page == prev_page %}dsbl_pg{% endif %}""><a href="{% if prev_page == 1 %}/pages/{{ page.handle }}{% else %}/pages/{{ page.handle }}/page-{{prev_page}}{% endif %}">Prev page</a></div>'+g+'<div class="next_page {% if c_page == max_page %}dsbl_pg{% endif %}"><a href="{% if next_page == 1 %}/pages/{{ page.handle }}{% else %}/pages/{{ page.handle }}/page-{{next_page}}{% endif %}">Next page</a></div>');
                               function pageNumbers(n, p) {
    n = Number(n);
    p = Number(p);
    var a = [];
    if (p >= 4) {
      a = [1];
    }
    for (var x = p - 2; x <= n; x++) {
      if (x < 1) continue;
      if (x < p) a.push(x);
      else if (x == p || x == p + 1 || x == p + 2) a.push(x);
      else {
        if (n - 1 < x) a.push(x);
      }
    }
    return a;
  }

</script>


