  <div class="prev_page {% if c_page == prev_page %}dsbl_pg{% endif %}"><a href="{% if prev_page == 1 %}/pages/{{ page.handle }}/search={{search_val}}{% else %}/pages/{{ page.handle }}/search={{search_val}}&page={{prev_page}}{% endif %}">Prev page</a></div>
  {% for i in (1..max_page) %}
  <div class="cs_pagination {% if i == c_page %}active{% endif %}"><a href="{% if i == 1 %}/pages/{{ page.handle }}/search={{search_val}}{% else %}/pages/{{ page.handle }}/search={{search_val}}&page={{i}}{% endif %}">{{i}}</a></div>
  {% endfor %}
  <div class="next_page {% if c_page == next_page %}dsbl_pg{% endif %}"><a href="{% if next_page == 1 %}/pages/{{ page.handle }}/search={{search_val}}{% else %}/pages/{{ page.handle }}/search={{search_val}}&page={{next_page}}{% endif %}">Next page</a></div>
</div>

