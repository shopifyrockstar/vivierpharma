        // For  increment & decrement button  //

        function increaseValue($this) {
        var input = $this.previousElementSibling;
        var max_value = parseInt(input.max, 10);
        var value = parseInt(input.value, 10);
        var inventory_checking = (input.getAttribute('inventory_checking'));
        var check_restriction = (input.getAttribute('check_restriction'));
        if(check_restriction == 'false'){
          value = isNaN(value) ? 0 : value;
          value++;
          input.value = value;
          update_qty();
        }else{
          max_value = isNaN(max_value) ? 0 : max_value;
          if(inventory_checking != 'no_limit'){
            if(value < max_value){
              value = isNaN(value) ? 0 : value;
              value++;
              input.value = value;
              update_qty();
            }
          }else{
            value = isNaN(value) ? 0 : value;
            value++;
            input.value = value;
            update_qty();
          }
        }

    }

  function decreaseValue($this) {
    var input = $this.nextElementSibling;
    var value = parseInt(input.value, 10);
    value = isNaN(value) ? 0 : value;
    value < 1 ? value = 1 : '';
    value--;
    input.value = value;
    update_qty();
  }

$('.qtydiv input[name="quantity"]').keyup(function(){
  update_qty();
});

function update_qty(){
  var count  = 0;
  $(".quantitytd input").each(function(){
    count = count + parseInt($(this).val());
  });
  if($(".update_btn").length){
    $(".update_btn .ttl_qty").remove();
    $(".update_btn").append("<span class='ttl_qty'> ("+count+")</span>");
  }
}


  // For  increment & decrement button //

  // add single item to cart
  function addProductToCart(variant_id,$this){
    var qty = $this.parentElement.parentElement.querySelector('[name="quantity"]').value;
    if(qty <=0){
      //       alert('Select Quantity');
      $($this).closest("tr").find(".qtygz").remove();
      $($this).closest("tr").find(".quantitytd").append("<span class='qtygz' style='color: red;'>Select Quantity</span>");
      var thss= $($this);
      setTimeout(function(){
        thss.closest("tr").find(".qtygz").remove();
      },2000);
      return;
    }
    var http = new XMLHttpRequest();
    var url = '/cart/add.js';
    var params = 'quantity='+qty+'&id='+variant_id;
    $this.innerHTML = loading_svg;
    http.open('POST', url, true);

    //Send the proper header information along with the request
    http.setRequestHeader('Content-type', 'application/x-www-form-urlencoded');

    http.onreadystatechange = function() {//Call a function when the state changes.
      if(http.readyState == 4 && http.status == 200) {
        //         console.log('added');
        $this.innerHTML = 'ADDED!';
        setTimeout(function(){
          $this.innerHTML = '{{addtocart_label}}';
        },2000);
      }
    }
    http.send(params);
    setTimeout(function(){
      $.ajax({
        type: 'GET',
        url: '/cart.js',
        success: function(response) {
          json_response = JSON.parse(response);
          //           console.log( 'response', json_response );
          for(var i= 0; i<json_response.items.length;i++){
            if(json_response.items[i].variant_id == variant_id){
              var qty = json_response.items[i].quantity;
              $($this).parent("td").find(".cart_qty").remove();
               $($this).parents(".custom_coll").find('.quantitytd').attr('cart_qty',qty);
              setTimeout(function(){
              var cart_qty = parseInt($($this).parents(".custom_coll").find('.quantitytd').attr('cart_qty'));
              var available_qty = parseInt($($this).parents(".custom_coll").find('.quantitytd').attr('available_qty'));
              var updated_qty = available_qty-cart_qty;
              $($this).parents(".custom_coll").find('.in_stock').text('In stock('+updated_qty+')')
              $($this).parents(".custom_coll").find('.quantitytd input').attr('max',updated_qty);
               $($this).parents(".custom_coll").find('.quantitytd input#number').val(0)
              },200)
              $($this).parent("td").append("<div class='cart_qty'>"+qty+" items are already in cart.</div>");
              
            }
          }
        },
        error: function(status) {
          console.warn( 'ERROR', status );
        }
      });
    },1000);
  }

  /*===== Add multiple items to cart =====*/
  function waio_update_cart(input_length, $this){
    $this.innerHTML = loading_svg;
    waio_update_cart_process(input_length, $this);
  }

  function waio_update_cart_process(input_length, $this){

    if(input_length >= 0){
      //       console.log(input_length);
      var elem = document.querySelectorAll('[name="quantity"]')[input_length];
      //       console.log(elem);
      input_length--;
      if(elem.value > 0){
        // add this to cart
        //         console.log(elem.value)
        var qty = elem.value;
        var variant_id=elem.getAttribute('data-variant');
        var http = new XMLHttpRequest();
        var url = '/cart/add.js';
        var params = 'quantity='+qty+'&id='+variant_id;
        http.open('POST', url, true);

        //Send the proper header information along with the request
        http.setRequestHeader('Content-type', 'application/x-www-form-urlencoded');

        http.onreadystatechange = function() {//Call a function when the state changes.
          if(http.readyState == 4 && http.status == 200) {
            waio_update_cart_process(input_length, $this);

          }
        }
        http.send(params);
      }
      else{
        waio_update_cart_process(input_length, $this);
      }
    }
    else{
      //       console.log('added');
      $this.innerHTML = '{{updateButton_label}}';
      document.getElementById('waioQof_update_msg').style.display="block";
      //       setTimeout(function(){
      //       	document.getElementById('waioQof_update_msg').style.display="none";
      //       },5000)
    }
  }

