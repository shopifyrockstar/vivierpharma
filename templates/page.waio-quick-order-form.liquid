  <td class="item" {%if qof_col_state == "disabled"%}data-hide{%endif%}>
    <img src="{{waio_product.featured_image|img_url:'master'}}">
  </td>

  {% when 'Product Name' %}
  <td {%if qof_col_state == "disabled"%}data-hide{%endif%}>
    <a class="cd-popup-trigger"  href="{{waio_product.url}}">{{waio_product.title}}</a>
    {%if qof.enableQuickView == true %}
    <span style="position: relative;float: right;display: inline-block;" data-tooltip="Description" class="tooltip-bottom" onclick="divshow(this)"><desc></desc></span>
       {%endif%}
     <noscript>
      {{waio_product|json}}
    </noscript>
  </td>

  {% when 'Price' %}
  <td class="pricetd" {%if qof_col_state == "disabled"%}data-hide{%endif%}><i class="spof_from"></i>{{waio_product.price_min| money}} - {{waio_product.price_max| money}}</td>

  {% when 'Compare at price' %}
  <td class="pricetd" {%if qof_col_state == "disabled"%}data-hide{%endif%}><i class="spof_from">From </i>{{waio_product.compare_at_price_min| money}}</td>

  {% when 'Quantity' %}
  <td class="quantitytd quantitytd1" {%if qof_col_state == "disabled"%}data-hide{%endif%} cart_qty="{{vc_count}}" available_qty="{{product_inventory}}">{% if show_Instock_inventory %}{%- if product.variants.first.inventory_management == blank or product.variants.first.inventory_policy == 'continue' %}{% else %}In stock({{product_inventory}}){% endif %}{% endif %}</td>
  {% when 'Product SKU' %}
  <td {%if qof_col_state == "disabled"%}data-hide{%endif%}></td>

  {% when 'Vendor' %}
  <td {%if qof_col_state == "disabled"%}data-hide{%endif%}>{{waio_product.vendor}}</td>

  {% when 'Product Type' %}
  <td {%if qof_col_state == "disabled"%}data-hide{%endif%}>{{waio_product.type}}</td>

  {% endcase %}
  {%endfor%}
  <td>
    <input class="mytext s_options" type="submit" pid="{{waio_product.id}}" ph="{{waio_product.handle}}"  data-vendor="{{waio_product.vendor}}" data-type="{{waio_product.type}}" value="Show Options" onclick="customFunction(this)" p_title="{{waio_product.title}}">
    <script class="script_variants">{{waio_product.variants|json}}</script>
  </td>
  <td  style="display:none;"><ul  class="variant_stock">{% for pro_var in waio_product.variants %}<li class="ul_list" id="{{pro_var.id}}" qty="{{pro_var.inventory_quantity}}"></li>{% endfor %}</ul></td>
</tr>
{% endif %}

{% endif %}

{%endfor%}

{% endif %}
</tbody>
</table>

