{% if apply_pagination == 'true' %}

{% assign prev_page = c_page | minus: 1 %}
{% assign next_page = c_page | plus: 1 %}
{% if prev_page == 0 %}
{% assign prev_page = 1 %}
{% endif %}
{% assign c_items = pagination | times: next_page %}
{% assign c_items_last = total_products | plus: pagination %}
{% assign remainder = total_products | modulo: pagination %}
{% if remainder == 0 %}
{% assign max_page = total_products | divided_by: pagination %}
{% else %}
{% assign max_page = total_products | divided_by: pagination | plus: 1 %}
{% endif %}
{% if c_items > c_items_last %}
{% assign next_page = next_page | minus: 1 %}
{% endif %}

